<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="com.tagfans.plugin.webviewScale"
        version="0.1.4">

  <name>serviceDiscovery</name>
  <description>Cordova plugin for serviceDiscovery</description>
  <engines>
    <engine name="cordova" version=">=3.4.0"/>
  </engines>
  <platform name="android">
  <config-file target="AndroidManifest.xml" parent="/manifest">
  </config-file>
    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="webviewScale">
        <param name="android-package" value="com.tagfans.plugin.webviewScale"/>
      </feature>
    </config-file>
	<source-file src="src/android/webviewScale.java" target-dir="src/com/tagfans/plugin/"/>
  </platform>

</plugin>
